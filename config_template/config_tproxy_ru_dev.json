{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },

  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9095",
      "external_ui": "/etc/sing-box/ui",
      "secret": "",
      "external_ui_download_url": "https://ghp.ci/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "🎯 Global Direct",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "path": "/etc/sing-box/cache.db",
      "store_fakeip": false
    }
  },

  "dns": {
    "servers": [
      {"tag": "proxyDns", "address": "https://dns.google/dns-query", "address_resolver": "defaultDns", "address_strategy": "ipv4_only", "detour": "🚀 Node Selection"},
      {"tag": "localDns", "address": "https://doh.yandex.ru/dns-query", "address_resolver": "defaultDns", "address_strategy": "ipv4_only", "detour": "🎯 Global Direct"},
      {"tag": "defaultDns", "address": "192.168.1.1", "address_strategy": "ipv4_only", "detour": "🎯 Global Direct"}
    ],
    "rules": [
      { "outbound": "any", "server": "defaultDns", "disable_cache": true },
      { "rule_set": ["geosite-chat", "geosite-github", "geosite-meta", "geosite-youtube", "geoip-discord", "geosite-onedrive", "geosite-microsoft", "geoip-google", "geosite-tiktok", "geoip-apple", "geoip-netflix", "geoip-telegram"], "server": "proxyDns" }
    ],
    "final": "defaultDns",
    "strategy": "ipv4_only"
  },

  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "0.0.0.0",
      "listen_port": 7895,
      "sniff": true,
      "sniff_override_destination": false
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "sniff": true,
      "users": []
    }
  ],

  "outbounds": [
    { "tag": "🚀 Node Selection", "type": "selector", "outbounds": ["🐸 Manual Switch", "🎯 Global Direct"] },
    { "tag": "📃 Custom domains list", "type": "selector", "outbounds": ["🚀 Node Selection", "🐸 Manual Switch", "🎯 Global Direct"] },
    { "tag": "📹 YouTube", "type": "selector", "outbounds": ["🚀 Node Selection", "🐸 Manual Switch", "🎯 Global Direct"] },
