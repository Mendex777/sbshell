    { "tag": "🐸 Manual Switch", "type": "selector", "outbounds": ["{all}"] },
    { "tag": "🇭🇰 Hong Kong Node", "type": "selector", "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇭🇰|HK|hk|Hong Kong|Hong|HongKong"] }] },
    { "tag": "🇯🇵 Japan Node", "type": "selector", "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇯🇵|JP|jp|Japan|Japan|Japan"] }] },
    { "tag": "🇺🇸 US Node", "type": "selector", "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇺🇸|US|us|United States|United|UnitedStates"] }, { "action": "exclude", "keywords": ["Hong Kong|Hong|HK|hk|HongKong"] }] },
    { "tag": "🔯 Hong Kong Auto", "type": "urltest", "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇭🇰|HK|hk|Hong Kong|Hong|HongKong"] }], "url": "http://www.gstatic.com/generate_204", "interval": "10m", "tolerance": 50 },
